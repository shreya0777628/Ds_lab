#include <iostream>
using namespace std;

int countInversions(int elements[], int length) {
    int totalInversions = 0;
    for (int i=0; i<length-1; i++) {
        for (int j=i+1; j<length; j++) {
            if (elements[i] > elements[j]) totalInversions++;
        }
    }
    return totalInversions;
}

int main() {
    int series[] = {2,4,1,3,5};
    int size = 5;
    cout << "Inversions: " << countInversions(series, size);
    return 0;
}
